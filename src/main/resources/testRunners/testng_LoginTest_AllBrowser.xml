<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="OpenCart Regression Suite" verbose="4" thread-count="4" parallel="tests">
	<test name="Login Page Test firefox">
		<parameter name="browser" value="firefox"/>
		<classes>
			<class name="com.qa.abc.tests.LoginTest"/>
		</classes>
	</test>
	<test name="Login Page Test chrome">
		<parameter name="browser" value="chrome"/>
		<classes>
			<class name="com.qa.abc.tests.LoginTest"/>
		</classes>
	</test>
	<test name="Login Page Test edge">
		<parameter name="browser" value="edge"/>
		<classes>
			<class name="com.qa.abc.tests.LoginTest"/>
		</classes>
	</test>
	
</suite>